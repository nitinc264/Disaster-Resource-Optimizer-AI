{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for Disaster Response Resource Optimization Platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "sms:simulate": "node scripts/simulate-sms.js",
    "sms:simulate2": "node scripts/simulate-sms2.js",
    "sms:simulate3": "node scripts/simulate-sms3.js",
    "sms:simulate4": "node scripts/simulate-sms4.js",
    "db:clear": "node scripts/clear-db.js --confirm",
    "monitor": "node scripts/monitor-data-changes.js",
    "agent:oracle": "node agents/oracle_agent.js",
    "agent:sentinel": "cd agents && node -e \"const p=process.platform==='win32'?'venv/Scripts/python':'venv/bin/python';require('child_process').execSync(p+' sentinel_agent.py',{stdio:'inherit'})\"",
    "agent:logistics": "cd agents && node -e \"const p=process.platform==='win32'?'venv/Scripts/python':'venv/bin/python';require('child_process').execSync(p+' logistics_agent.py',{stdio:'inherit'})\""
  },
  "keywords": [],
  "author": "Omkar Kale",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "cloudinary": "^2.9.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.1",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.14.0",
    "twilio": "^5.10.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "eslint": "^10.0.1",
    "nodemon": "^3.0.1"
  }
}
